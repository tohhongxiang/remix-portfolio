    "@remix-run/serve": "^2.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "husky": "^9.0.11",
    "isbot": "^4.1.0",
    "lint-staged": "^15.2.2",
    "lucide-react": "^0.334.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
